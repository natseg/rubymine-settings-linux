<templateSet group="JavaScript Testing">
  <template name="desc" value="describe('$description$', () =&gt; {&#10;  $END$&#10; });" description="describe" toReformat="false" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ete" value="expect($actual$).toEqual($expected$);" description="expect to equal" toReformat="false" toShortenFQNames="true">
    <variable name="actual" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="expected" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="it" value="it('then $behaviour_description$', function(){&#10;  $END$&#10;});" description="it" toReformat="false" toShortenFQNames="true">
    <variable name="behaviour_description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sdesc" value="describe('$description$', function(){&#10;  $SELECTION$&#10; });" description="surround with describe" toReformat="false" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="be" value="beforeEach(function() {&#10;  $END$&#10;});" description="beforeEach" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="con" value="context('$DESC$', function() {&#10;&#10;  $END$  &#10;  &#10;});" description="test context" toReformat="true" toShortenFQNames="true">
    <variable name="DESC" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ita" value="it('then $behaviour_description$', async function(){&#10;&#10;  $END$&#10;  &#10;});" description="it async" toReformat="false" toShortenFQNames="true">
    <variable name="behaviour_description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ncm" value="get $name$() {&#10;  return this.newComponent(By.css('$locator$')$END$);&#10;}" description="new component method" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="locator" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_EXPRESSION" value="false" />
      <option name="JS_STATEMENT" value="false" />
    </context>
  </template>
  <template name="bef" value="before(function() {&#10;  $END$&#10;});" description="before block" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="befa" value="before(async function() {&#10;  $END$&#10;});" description="async before block" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>