<templateSet group="JavaScript React Imports">
  <template name="lodash" value="import _ from 'lodash';" description="import lodash" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="reflux" value="import Reflux from 'reflux';" description="import reflux" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="router" value="import { $component$ } from 'react-router';" description="react router" toReformat="false" toShortenFQNames="true">
    <variable name="component" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="anim" value="import ReactCSSTransitionGroup from 'react-addons-css-transition-group';" description="React animation include" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="purecomponent" value="'use strict';&#10;&#10;import React, { PropTypes } from 'react';&#10;&#10;const $COMPONENT_NAME$ = ({ $PROPS$ })=&gt; {&#10;&#10;};&#10;&#10;$COMPONENT_NAME$.propTypes = {&#10;&#10;};&#10;&#10;$COMPONENT_NAME$.defaultProps = {&#10;&#10;};&#10;&#10;module.exports = $COMPONENT_NAME$;&#10;" description="React Pure Component" toReformat="false" toShortenFQNames="true">
    <variable name="COMPONENT_NAME" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="false" />
    <variable name="PROPS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="rrpt" value="import { $propType$ } from 'react-router/lib/PropTypes';" description="react router prop types" toReformat="false" toShortenFQNames="true">
    <variable name="propType" expression="enum(&quot;routerShape&quot;, &quot;locationShape&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impl" value="import $import$ from './$path$';" description="import current directory" toReformat="true" toShortenFQNames="true">
    <variable name="path" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="import" expression="" defaultValue="path" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impstyle" value="import style from './$path$$END$.less';" description="import style" toReformat="true" toShortenFQNames="true">
    <variable name="path" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impu" value="import $obj2$ from '../$path$$obj$';" description="import up directory" toReformat="true" toShortenFQNames="true">
    <variable name="path" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="obj" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="obj2" expression="" defaultValue="obj" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impsp" value="import { $attrs$ } from '$obj$';" description="splat" toReformat="false" toShortenFQNames="true">
    <variable name="obj" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="attrs" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="materialui" value="import $comp2$ from 'material-ui/$comp$';" description="maturialui" toReformat="false" toShortenFQNames="true">
    <variable name="comp" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="comp2" expression="" defaultValue="comp" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impli" value="import $const$ from '$library$';" description="import libray" toReformat="false" toShortenFQNames="true">
    <variable name="const" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="library" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cm" value="/*&#10; * $comment$$END$&#10; */" description="comment" toReformat="true" toShortenFQNames="true">
    <variable name="comment" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="hel" value="import Helmet from 'react-helmet';" description="react helmet" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="materialuiicon" value="import $iconName$ from 'material-ui/svg-icons/$directory$/$file$';" description="material icon" toReformat="false" toShortenFQNames="true">
    <variable name="directory" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="file" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="iconName" expression="concat(capitalize(camelCase(file)), &quot;Icon&quot;)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="bluebird" value="import Promise from 'bluebird';" description="bluebird" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impsub" value="import $FILE2$ from './$PATH$$FILE$';" description="import subdirectory" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FILE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FILE2" expression="" defaultValue="FILE" alwaysStopAt="false" />
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impfs" value="import fontStyle from '../$PATH$/fonts.less';" description="import fontStyle" toReformat="true" toShortenFQNames="true">
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>